function proxyup {
  export HTTP_PROXY=http://proxygsprimary.trghosting.net:3128
  export HTTPS_PROXY=https://proxygsprimary.trghosting.net:3128
  export RSYNC_PROXY=proxygsprimary.trghosting.net:3128
  export http_proxy=$HTTP_PROXY
  export https_proxy=$HTTPS_PROXY
  export rsync_proxy=$RSYNC_PROXY
  export npm_config_proxy=$HTTP_PROXY
  export npm_config_https_proxy=$HTTPS_PROXY
}

function proxydown {
  unset HTTP_PROXY
  unset HTTPS_PROXY
  unset RSYNC_PROXY
  unset http_proxy
  unset https_proxy
  unset rsync_proxy
  unset npm_config_proxy
  unset npm_config_https_proxy
}

function maven2 {
  export CURRENT_MAVEN_HOME=$M2_HOME
  export CURRENT_PATH=$PATH
  export M2_HOME=/me/prj/wgsn/tools/apache-maven/apache-maven-2.2.1
  export PATH=/me/prj/wgsn/tools/apache-maven/apache-maven-2.2.1/bin:$PATH
}

# Assumes shell always start from maven 3.0.3 default
function maven3 {
  export M2_HOME=$CURRENT_MAVEN_HOME
  export PATH=$CURRENT_PATH
  unset CURRENT_MAVEN_HOME
  unset CURRENT_PATH
}

function java6 {
  export CURRENT_JAVA_HOME=$JAVA_HOME
  export JAVA_HOME=/Library/Java/Home
}

function java7 {
  export JAVA_HOME=$CURRENT_JAVA_HOME
  unset CURRENT_JAVA_HOME
}
